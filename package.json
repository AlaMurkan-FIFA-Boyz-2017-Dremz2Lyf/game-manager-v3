{
  "name": "react-micro-app-starter-kit",
  "version": "1.0.0",
  "description": "A Starter Kit for a React UI Project",
  "main": "index.js",
  "scripts": {
    "test": "better-npm-run test",
    "test:int": "better-npm-run test:int",
    "test:watch": "better-npm-run test:watch",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint . ",
    "lint:css": "stylelint ./src/app/**/*.css",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "codecov": "codecov",
    "build": "npm run build:clean && better-npm-run build",
    "build:clean": "npm run test:clean && rimraf ./client",
    "start": "better-npm-run start:service",
    "clean:coverage": "rimraf ./coverage"
  },
  "betterScripts": {
    "test": {
      "command": "karma start ./tools/testing/karma.conf.js",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:int": {
      "command": "karma start ./tools/testing/karma-int.conf.js",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:watch": {
      "command": "karma start ./tools/testing/karma.conf.js --no-single-run --auto-watch",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "build": {
      "command": "webpack --config ./tools/webpack/prod.config.js --progress --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start:service": {
      "command": "webpack-dev-server --config ./tools/webpack/dev-server.config.js",
      "env": {
        "HOT_RELOAD": "true",
        "NODE_ENV": "development"
      }
    }
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.css": "stylelint"
  },
  "pre-commit": "lint:staged",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/danteinc-tutorial/react-starter-kit.git"
  },
  "keywords": [
    "react",
    "ui"
  ],
  "author": "Dante",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://gitlab.com/danteinc-tutorial/react-starter-kit/issues"
  },
  "homepage": "https://gitlab.com/danteinc-tutorial/react-starter-kit#README",
  "dependencies": {
    "babel-plugin-react-css-modules": "2.8.0",
    "babel-plugin-transform-runtime": "6.23.0"
  },
